{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended",
        "tslint-consistent-codestyle",
        "tslint-config-airbnb"
    ],
    "linterOptions": {
        "project": "tsconfig.json",
        "exclude": [
            "node_modules",
            "src/simpleData.ts"
        ]
    },
    "jsRules": {},
    "rules": {
        "no-duplicate-variable": true,
        "import-name": false,
        "object-literal-sort-keys": false,
        "no-namespace": false,
        "interface-name": false,
        "max-line-length":false,
        "no-bitwise":false
    },
    "rulesDirectory": [
        "./src/"
    ]
}